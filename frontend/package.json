    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.3.2",
    "axios": "^0.24.0",
    "google-libphonenumber": "^3.2.27",
    "history": "5",
    "http-status-codes": "^2.2.0",
    "ky": "^0.28.7",
